[push]
  default = current
[alias]
  co = checkout
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  rb = rebase
  br = branch
  bd = branch -d
  ci = commit
  ro = !git fetch && git rebase origin/master
  ri = !git fetch && git rebase -i origin/master
  ft = fetch
  st = status
  df = diff
[rebase]
  autosquash = true
